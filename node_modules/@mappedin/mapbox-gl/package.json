{
	"name": "@mappedin/mapbox-gl",
	"version": "1.2.7",
	"main": "dist/mapbox-gl.js",
	"esnext": "dist/mapbox-gl.js",
	"module": "dist/mapbox-gl.js",
	"esm": true,
	"browser": {
		"./_mapbox-gl/src/shaders/index.js": "./_mapbox-gl/src/shaders/shaders.js",
		"./_mapbox-gl/src/util/window.js": "./_mapbox-gl/src/util/browser/window.js",
		"./_mapbox-gl/src/util/web_worker.js": "./_mapbox-gl/src/util/browser/web_worker.js"
	},
	"license": "MIT",
	"scripts": {
		"download": "rm -rf ./_mapbox-gl && node download && cd ./_mapbox-gl && yarn",
		"build-dev": "rollup -c rollup.config.js --environment BUILD:dev",
		"watch-dev": "rollup -c rollup.config.js --environment BUILD:dev --watch",
		"build-prod": "rollup -c rollup.config.js --environment BUILD:production",
		"build-prod-min": "rollup -c rollup.config.js --environment BUILD:production,MINIFY:true",
		"build-csp": "rollup -c rollup.config.csp.js"
	},
	"files": [
		"dist/"
	],
	"devDependencies": {
		"@mapbox/flow-remove-types": "^1.3.0-await.upstream.2",
		"@mapbox/mapbox-gl-supported": "1.4.1",
		"@mapbox/point-geometry": "^0.1.0",
		"@rollup/plugin-alias": "^2.2.0",
		"@rollup/plugin-replace": "^2.2.1",
		"github-download": "^0.5.0",
		"json": "^9.0.6",
		"rollup": "^1.23.1",
		"rollup-plugin-buble": "^0.19.8",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-sourcemaps": "^0.4.2",
		"rollup-plugin-terser": "^5.1.2",
		"rollup-plugin-unassert": "^0.3.0",
		"rollup-plugin-visualizer": "^3.3.0"
	}
}
