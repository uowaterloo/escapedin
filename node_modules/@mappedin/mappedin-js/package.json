{
  "name": "@mappedin/mappedin-js",
  "version": "1.65.6",
  "description": "Mappedin 3D map javascript component",
  "main": "builds/mappedin.js",
  "module": "modules/Mappedin-modules.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --config=jest.config.json --watchAll",
    "coverage": "cross-env NODE_ENV=test jest --config=jest.config.json --coverage --verbose",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --config webpack.config-modules.js --optimize-minimize --mode=production",
    "build": "npm-run-all clean build-web build-node build-modules",
    "build-web": "cross-env NODE_ENV=production webpack --optimize-minimize --mode=production --progress --colors",
    "build-modules": "cross-env NODE_ENV=production rollup -c",
    "build-modules-debug": "cross-env NODE_ENV=dev rollup -c --watch",
    "build-debug": "webpack --progress --colors",
    "build-web-debug": "webpack --progress --colors --watch --mode=development",
    "build-demo-debug": "webpack --progress --colors --config webpack.config-demo.js --mode=development",
    "build-demo": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config-demo.js --optimize-minimize --mode=production ",
    "build-demo-dualview": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config-demo-dualview.js --optimize-minimize --mode=production",
    "build-demo-multifloor": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config-demo-multifloor.js --optimize-minimize --mode=production",
    "build-test-screenshot": "cross-env NODE_ENV=production TESTING=true webpack --progress --colors --config webpack.config-test-screenshot.js --mode=production",
    "clean": "rimraf builds",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --colors --inline --config webpack.config-demo.js",
    "dev-multifloor": "cross-env NODE_ENV=dev webpack-dev-server --progress --colors --inline --config webpack.config-demo-multifloor.js",
    "dev-dualview": "cross-env NODE_ENV=dev webpack-dev-server --progress --colors --inline --config webpack.config-demo-dualview.js",
    "dev-bluedot": "cross-env NODE_ENV=dev webpack-dev-server --progress --colors --inline --config webpack.config-demo-blue-dot.js",
    "build-node": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config-node.js",
    "build-node-debug": "webpack --progress --colors --config webpack.config-node.js --mode=development",
    "build-docs": "cross-var yuidoc -C --project-version $npm_package_version .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "screenshot-test-server": "http-server -p 43212",
    "screenshot-test-server-debug": "cross-env NODE_ENV=dev webpack-dev-server --progress --colors --inline --config webpack.config-test-screenshot.js",
    "cypress-open": "cypress open",
    "cypress-run": "cypress run --browser chrome",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MappedIn/mapview-js.git"
  },
  "keywords": [
    "mappedin",
    "mapview"
  ],
  "author": "Paul Bernhardt <paul@mappedin.ca>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/MappedIn/mapview-js/issues"
  },
  "homepage": "https://github.com/MappedIn/mapview-js#readme",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@mappedin/mapbox-gl": "^1.2.6",
    "@tweenjs/tween.js": "^17.2.0",
    "@types/jest": "^26.0.3",
    "autoprefixer": "^9.4.10",
    "balance-text": "3.2.1",
    "canvas-txt": "^2.0.10",
    "color-scheme": "^1.0.1",
    "fastdom": "^1.0.9",
    "geodesy": "^1.1.3",
    "js-clipper": "^1.0.1",
    "jszip": "^3.4.0",
    "kalmanjs": "^1.1.0",
    "leaflet": "^1.3.1",
    "leaflet-rotatedmarker": "^0.2.0",
    "libarchive.js": "^1.3.0",
    "lodash.defaults": "^4.2.0",
    "memoized-class-decorator": "^1.6.1",
    "node-sass": "^4.11.0",
    "p2": "^0.7.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "three": "0.107.0",
    "tinycolor2": "^1.4.1",
    "word-wrapper": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.0.0",
    "@types/jest": "^26.0.8",
    "array-unique": "^0.3.2",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.4",
    "btoa": "^1.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "core-js": "^2.6.9",
    "cross-env": "^5.1.4",
    "cross-var": "^1.1.0",
    "css-loader": "^3.2.0",
    "cypress": "^3.4.1",
    "cypress-plugin-snapshots": "^1.2.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^3.1.2",
    "exports-loader": "^0.6.3",
    "file-loader": "^1.1.11",
    "gm": "^1.23.1",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^26.0.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-fetch-mock": "^1.5.0",
    "jest-junit": "^3.7.0",
    "json-loader": "^0.5.7",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.2",
    "postcss-url": "8.0.0",
    "prettier": "^1.18.2",
    "query-string": "^5.1.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.18.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-glsl": "^1.3.0",
    "rollup-plugin-image-base64": "anzorb/rollup-plugin-image-base64",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^1.0.1",
    "rollup-plugin-sizes": "^0.5.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svgo": "^1.0.3",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-url": "^3.0.1",
    "shader-loader": "^1.3.1",
    "shave": "^2.5.2",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "url-loader": "^0.6.2",
    "uuid": "3.1.0",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1",
    "whatwg-fetch": "^2.0.4",
    "xmlhttprequest": "^1.8.0",
    "yuidocjs": "^0.10.2"
  },
  "files": [
    "builds/",
    "modules/"
  ],
  "jest": {
    "testEnvironment": "node"
  }
}
